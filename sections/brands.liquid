{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  
{%- endstyle -%}
<div class="section-{{ section.id }}-padding color-{{ section.settings.color_scheme }} gradient">
<div class="announcement-bar--inner-{{ section.id }} direction-left " style="--marquee-speed: {{ section.settings.scroll_speed }}s">
     <div style="display: flex;">
       <div style="display: flex;">
        {%- for block in section.blocks -%}
          {% case block.type %}
          {% when 'image' %}
            <style>
              .announcement-bar-{{ block.id }} img {
    width: {{block.settings.logo_image_width}}px;
    height:auto;
  }
               .announcement-bar-{{ block.id }} svg {
    width: {{block.settings.logo_image_width}}px;
    height:auto;
  }

  @media (max-width: 749px) {

    .announcement-bar-{{ block.id }} img {
      width: {{ block.settings.logo_image_height_mobile }}px;
      height:auto;
    }
                 .announcement-bar-{{ block.id }} svg {
      width: {{ block.settings.logo_image_height_mobile }}px;
      height:auto;
    }
  }
            </style>
        <div class="announcement-bar-{{ section.id }} announcement-bar-{{ block.id }}" role="region" aria-label="Announcement" {{ block.shopify_attributes }}>
          {% if block.settings.logo_image != blank %}
              {{
            block.settings.logo_image
            | image_url: width: 200
            | image_tag: loading: 'lazy'
          }}
             {%- else -%}
              <p style="font-size: 40px;margin: 0;">LOGO</p>
            {%- endif -%}
                  </div>
            
           {% when 'announcement' %}
             <style>
        .lumin-text-{{ block.id }} {
          color: {{ block.settings.announcement_color }};
          font-size: {{ block.settings.announcement_size }}px;
        }
      @media screen and (max-width: 749px) {
         .lumin-text-{{ block.id }} {
          font-size: {{ block.settings.announcement_size_m }}px;
        }
      
      </style>
        <div class="announcement-bar-{{ section.id }}" role="region" aria-label="Announcement" {{ block.shopify_attributes }}>
              <a {% if block.settings.text_link != blank %}{{ block.settings.text_link }}{% else %} href="/collections/frontpage"{% endif %} class="announcement-bar__link link link--text focus-inset animate-arrow">
                {% render 'icon-accordion', icon: block.settings.icon %}  
                <p class="announcement-bar__message h6 lumin-text-{{ block.id }}">
                       {{ block.settings.Announcement }}
                      </p>
                </a>
                  </div>
          {% endcase %}
        {% endfor %}
      </div>
     </div>
