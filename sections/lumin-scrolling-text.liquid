{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}
<div class="section-{{ section.id }}-padding">
  <div
    class="sw-announcement announcement-bar--inner-{{ section.id }} {% if section.settings.reverse %} direction-right{% else %} direction-left{% endif %} color-{{ section.settings.color_scheme }} gradient"
    style="--marquee-speed: {{ section.settings.scroll_speed }}s"
  >
     <div style="display: flex;">
      <div style="display: flex;">
        {%- for block in section.blocks -%}
          {% case block.type %}
            {% when 'image' %}
              <style>
                            .announcement-bar-{{ block.id }} img {
                  width: {{block.settings.logo_image_width}}px;
                  height:auto;
                }
                             .announcement-bar-{{ block.id }} svg {
                  width: {{block.settings.logo_image_width}}px;
                  height:auto;
                }
              </style>
              <div
                class="announcement-bar-{{ section.id }} announcement-bar-{{ block.id }}"
                role="region"
                aria-label="Announcement"
                {{ block.shopify_attributes }}
              >
                {% if block.settings.logo_image != blank %}
                  {{ block.settings.logo_image | image_url: width: 200 | image_tag: loading: 'lazy' }}
                {%- else -%}
                  <p style="font-size: 40px;margin: 0;">Brand</p>
                {%- endif -%}
              </div>

            {% when 'announcement' %}
              <style>
                  .lumin-text-{{ block.id }} p {
                    color: {{ block.settings.announcement_color }};
                    font-size: {{ block.settings.announcement_size }}px;
                    letter-spacing: {{ block.settings.text_spacing }}px;
                    text-transform: {{ block.settings.text_transform }};
                    margin:0;
                  }
                @media screen and (max-width: 749px) {
                   .lumin-text-{{ block.id }} p {
                    font-size: {{ block.settings.announcement_size_m }}px;
                  }
                }
                   .lumin-text-{{ block.id }} svg{
                    width:calc({{ block.settings.announcement_size }}px + 2px);
                    height:calc({{ block.settings.announcement_size }}px + 2px);
                  }
                 @media screen and (max-width: 749px) {
                  .lumin-text-{{ block.id }} svg{
                    width:{{ block.settings.announcement_size_m }}px;
                    height:{{ block.settings.announcement_size_m }}px;
                  }
                }
              </style>
              <div
                class="lumin-text-{{ block.id }} announcement-bar-{{ section.id }}"
                role="region"
                aria-label="Announcement"
                {{ block.shopify_attributes }}
              >
                <span class="scroll rte">
                  {% if block.settings.icon == 'custom' %}
                    {{ block.settings.custom_icon }}
                    {% else  %}
                    {% render 'icon-accordion', icon: block.settings.icon %}
                  {% endif %}
                  {{ block.settings.Announcement }}
                </span>
              </div>
          {% endcase %}
        {% endfor %}
      </div>
    </div> 
