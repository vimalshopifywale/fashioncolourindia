  /* Pre-loader
  ============================= */
  
[data-Lumin-slideshow-section-id="{{ section.id }}"].LuminSlideshow-container--loading:before{
    left: 0;
    right: 0;
    content: '';
    top: 0;
    bottom: 0;
    background: {{ loader_background_color }};
    width: 100%;
    height: 100%;
    z-index: 1;
    position: absolute;
  }


  .LuminSlideshow-container--loading:after{
    opacity: 0;
    animation: preloading .5s ease .3s infinite;
    animation-delay: 1s;
    content: "";
    position: absolute;
    width: 100px;
    height: 3px;
    background: white;
    left: 50%;
    top: 50%;
    margin: -1px 0 0 -50px;
    z-index: 2;
  }
  
